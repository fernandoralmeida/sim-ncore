

<!DOCTYPE html>
<html lang="pt-br">    
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="Content-Language" content="pt-br">
    <link rel="shortcut icon" href="~/favicon.ico" />
    <link rel="stylesheet" href="~/lib/google/icons.css"/>
    <link rel="stylesheet" href="~/lib/materialize/css/materialize.min.css" />    
    <link rel="stylesheet" href="~/css/site.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SIM - @ViewData["Title"]</title>
</head>
<body class="has-fixed-sidenav">
    <header>
        <partial name="_LoginPartial" />
        <partial name="_SidebarNav" />        
    </header>
    <main>        
        <div class="progress" style="display: flex; position: fixed; margin-top: -4px; z-index:998; border-radius: 0px;"><div class="indeterminate"></div></div>
        <div class="blackboxload"></div>
        <div class="teal lighten-2 navbar-extended"></div>         
        <div class="container-m20">            
            @RenderBody()
        </div>
    </main>
    <footer class="page-footer teal lighten-2">
        <div class="container">
            <div class="row">
                <div class="col l6 s12">
                    <h6 class="white-text">Servidores</h6>
                    <ul>
                        <li><a class="alpha-w-text-9" href="https://portal.conam.com.br/rhjau/login.php" target="_blank">Meu Holerite</a></li>
                        <li><a class="alpha-w-text-9" href="http://www.jau.sp.gov.br/arquivos/manual_do_servidor.pdf" target="_blank">Manual do Servidor Público</a></li>
                        <li><a class="alpha-w-text-9" href="http://pontodigital.jau.sp.gov.br/" target="_blank">Ponto Digital</a></li>
                        <li><a class="alpha-w-text-9" href="http://webmail.jau.sp.gov.br/" target="_blank">Web Mail</a></li>
                    </ul>
                </div>
                <div class="col l4 offset-l2 s12 right">
                    <h6 class="white-text">Links</h6>
                    <ul>
                        <li><a class="alpha-w-text-9" href="https://jau.sp.gov.br/servicos-online" target="_blank">Serviços Online</a></li>
                        <li><a class="alpha-w-text-9" href="https://jau.sp.gov.br/jornal-oficial" target="_blank">Jornal Oficial</a></li>
                        <li><a class="alpha-w-text-9" href="https://leideacesso.etransparencia.com.br/jahu.prefeitura.sp" target="_blank">Transparencia Pública</a></li>
                        <li><a class="alpha-w-text-9" href="http://leis.jau.sp.gov.br/" target="_blank">Legislação Municipal</a></li> 
                    </ul>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            <div class="container white-text">
                &copy; @DateTime.Now.Year - Sim.Web
                <a class="white-text right" href="http://www.jau.sp.gov.br" target="_blank">Jahu-SP</a>
            </div>
        </div>
    </footer>

    <script type="text/javascript" src="~/lib/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="~/js/site.js"></script>
    <script type="text/javascript" src="~/lib/materialize/js/materialize.min.js"></script>
    
    <script>
        $(window).on('load', function () { 
            $(".progress").fadeOut("slow"); 
            $(".blackboxload").hide();
        });    
        $(function () {
            $(".submit").click(function () {
                $(".blackboxload").show();
                $(".progress").show();
            });
        });
        function handle(e){
        if(e.keyCode === 13){
            var v = $("#search").val();
            if(v === "")
                return false;

            $(".blackboxload").show();
            $(".progress").show();
        }
    }
    </script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
